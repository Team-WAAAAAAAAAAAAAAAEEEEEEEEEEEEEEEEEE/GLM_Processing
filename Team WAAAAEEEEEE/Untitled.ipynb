    "def print_stuff(n):\n",
    "    i = 1\n",
    "    while i <= n:\n",
    "        if (i % 3) + (i % 5) == 0:\n",
    "            print(\"fizzbuzz\")\n",
    "            i+=1\n",
    "            continue\n",
    "        if i % 3 == 0:\n",
    "            print(\"fizz\")\n",
    "        if i % 5 == 0:\n",
    "            print(\"buzz\")\n",
    "        i+=1\n",
    "print_stuff(15)"
   ]
